<div class='container'>
<div class="row">
 <div class="col-md-12 bg-white">
<h1 class="text-dark">全ユーザー一覧</h1>

<ul>
  <% @users.each do |user| %>
   <li>
     <%= user.name %> - <%= user.email %>
     <%= link_to '削除', admin_user_path(user), method: :delete, date: { confirm: '本当に削除しますか?'} %>
   </li>
  <% end %>
</ul>
</div>
</div>
</div>